<main>
<h1 class="bg-light">商品一覧</h1>
<div class="continer">
  <div class="row">
<table width="90%", class="mx-auto">
<tr class="bg-light">
  <th >商品ID</th>
  <th >商品名</th>
  <th >税抜価格</th>
  <th >ジャンル</th>
  <th >ステータス</th>
</tr>
<tr>
<% @items.each do |item| %>
<td><%= item.id %></td>
<td><%= link_to item.name , admins_item_path(item.id) %></td>
<td><%= item.price %></td>
<td>ジャンル</td>
<td>ステータス</td>
</tr>
<% end %>
</table>
<p class="text-right"><%= link_to "商品新規登録画面へ", new_admins_item_path %></p><br>
</div>
</div>
</main>