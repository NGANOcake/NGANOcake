<main class="continer">
  <div class="row">
    <div class="mx-auto">
      <h1>商品新規登録</h1>
<%= form_with model: @item, url: admins_items_path do |f| %>
<h4>商品画像</h4>
<%= f.file_field :image, accept: "image/*" %>
<h4>商品名</h4>
<%= f.text_field :name %>
<h4>商品説明</h4>
<%= f.text_area :introduction %>
<h4>ジャンル</h4>
<p>選択してください</p>
<h4>税抜価格</h4>
<%= f.text_field :price %>
<h4>販売ステータス</h4>
<p>〇 ●</p>
<%= f.submit "新規登録" %>
<% end %>
    </div>
  </div>
</main>